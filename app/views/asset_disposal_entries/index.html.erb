<p id="notice"><%= notice %></p>

<h1>Asset Disposal Entries</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th>Unit</th>
      <th>Amount</th>
      <th>Disposeamount</th>
      <th>Asset seat</th>
      <th>Disposeprice</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @asset_disposal_entries.each do |asset_disposal_entry| %>
      <tr>
        <td><%= asset_disposal_entry.Code %></td>
        <td><%= asset_disposal_entry.name %></td>
        <td><%= asset_disposal_entry.Unit %></td>
        <td><%= asset_disposal_entry.Amount %></td>
        <td><%= asset_disposal_entry.DisposeAmount %></td>
        <td><%= asset_disposal_entry.Asset_seat %></td>
        <td><%= asset_disposal_entry.DisposePrice %></td>
        <td><%= link_to 'Show', asset_disposal_entry %></td>
        <td><%= link_to 'Edit', edit_asset_disposal_entry_path(asset_disposal_entry) %></td>
        <td><%= link_to 'Destroy', asset_disposal_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Asset Disposal Entry', new_asset_disposal_entry_path %>
